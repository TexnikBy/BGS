{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "es6",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node"
    },
    "include": [
        "src/**/*.tsx",
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}